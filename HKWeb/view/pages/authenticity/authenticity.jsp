function scrollItem(elm){
  var offset = elm.data('offset');//offset value for element, to start animation
  //var eHeight = elm.height();
  //var pHeight = $('#section2').height();
  var tHeight = elm.data('textArea');
  var wHeight = $(window).height();
  var elmPos = elm.data('top'); // actual postion of element from top
  var scrollTop = $(window).scrollTop(); // current scroll position of browser
  //var threshold = elm.data('shiftLimit');
  var marginTop = 0;

  if(wHeight + scrollTop - offset > elmPos){
    marginTop = wHeight + scrollTop - offset - elmPos; //possible new position
    if(marginTop < tHeight){
      //elm.siblings('.msg').hide();
      elm.stop(true,true).animate({'margin-top':marginTop});
    } else {

      elm.stop(true,true).animate({'margin-top': tHeight});
      if(elm.siblings('.msg').find('div').animate({'right':'-1500px'},10).animate({'right':'0px','opacity':'1'},500)){
        elm.siblings('.msg').find('div').find('span').addClass('red-color');
        setTimeout(function(){$('.box1').css({'background-image':$('.box1').css('background-image').replace(/dibba\.png/g,'dibba-blue.png')});
          $('.box2').css({'background-image':$('.box2').css('background-image').replace(/dibba\.png/g,'dibba-red.png')})},400);
      }
      //tells animation is complete
      return true;
    }
  }
  //tells animation is yet not complete
  return false;

};

//handling scroll event for top boxes	
