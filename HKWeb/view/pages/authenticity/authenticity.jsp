$.fn.rotate = function(options){
  var settings = $.extend({'angle':'7deg'},options);

  function getIntegerFromAngle(angle){
    return Number(angle.replace('deg',''));
  }

  function getAngleFromInteger(val){
    return val+'deg';
  }

  function getInitialStepAngle(angle){
    return angle*.01
  }

  function getIntermediateStep(){

  }

  return this.each(function(){
    var self = $(this);
    var initialAngle = 0;
    var finalAngle = getIntegerFromAngle(settings.angle);
    var step = getInitialStepAngle(finalAngle);
    console.log(finalAngle);
    var rotation = function(){
      if(finalAngle<0){
        return function(){
          self.css({'transform':'rotate('+getAngleFromInteger(initialAngle)+')'});
          initialAngle = initialAngle + step;
          if( initialAngle > finalAngle ){
            setTimeout(rotation,5);
          }
        }
      } else {
        return function(){
          self.css({'transform':'rotate('+getAngleFromInteger(initialAngle)+')'});
          initialAngle = initialAngle + step;
          if( initialAngle < finalAngle ){
            setTimeout(rotation,5);
          }
        }
      }
    }();
    setTimeout(rotation,70);

  });
};

$.fn.heer = function(options){
  var settings = $.extend({
    HOLDER : '[data-role=holder]',
    HELD : '[data-role=held]',
    HELD_IDENTIFIER : 'data-identifier=',
    HOLDER_INITIATE : 'holderInitiate',
    HELD_INITIATE : 'heldInitiate',
    ANIMATION_SLOW : 1500,
    ANIMATION_NORMAL : 1000,
    ANIMATION_FAST : 500
  },options);

  /*
   * name : triggerEvent
   * type : function
   * purpose : trigger event for element if conditions satisfied
   */
  function triggerEvent(elm){
    var offset = elm.data('offset')?elm.data('offset'):100;
    var wHeight = $(window).height();
    var scrollTop = $(window).scrollTop();
    var elmPos = elm.offset().top;
    if(wHeight + scrollTop - offset > elmPos){
      elm.trigger(settings.HOLDER_INITIATE);
    }
  }

  /*
   * name : getHeldTargetString
   * type : function
   * purpose : return selector string for a given identifier
   */
  function getHeldTargetString(identifier){
    //identifier should be number
    return '['+settings.HELD_IDENTIFIER+identifier+']'
  }

  /*
   * name : getDelay
   * type : function
   * purpose : return delay counter for an element
   */
  function getDelay(elm){
    var delay = elm.data('delay');
    if(typeof delay !== "undefined" && typeof Number(delay) === "number"){
      return delay;
    }
    return 0
  }

  /*
   * name : getHinder
   * type : function
   * purpose : return animation type
   */
  function getHinder(elm){
    var hinder = elm.data('hinder');
    if(typeof hinder !== "undefined" && typeof hinder === "string"){
      return hinder;
    }
    return 'show'
  }

  $(settings.HOLDER).on(settings.HOLDER_INITIATE,function(){
    var target = getHeldTargetString($(this).data('target'));
    $(this).off(settings.HOLDER_INITIATE);
    $(this).find(target).trigger(settings.HELD_INITIATE);
  });

  $(settings.HELD).on(settings.HELD_INITIATE,function(){
    var target = $(this).data('target');
    var delay = getDelay($(this));
    var hinder = getHinder($(this));
    var self = $(this);
    console.log(hinder);
    setTimeout(function(){
      if(hinder==='rotate'){
        self[hinder]({'angle':self.data('hinderAngle')});
      } else if(hinder==='css'){
        self.animate({'margin-top':'50px'},500);
      } else {
        self[hinder](settings.ANIMATION_NORMAL);
      }

      if(typeof target !== "undefined"){
        target = getHeldTargetString(self.data('target'));
      } else {

      }
      self.parents(settings.HOLDER).find(target).trigger(settings.HELD_INITIATE);
    },delay);

  });

  return this.each(function(){
    var self = $(this);
    $(window).scroll(function(){
      triggerEvent(self);
    });
  });
};

